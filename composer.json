{
    "name": "orange-hive/simplyment",
    "description": "Simplified Development for TYPO3 developers",
    "type": "typo3-cms-extension",
    "version": "1.3.1",
    "license": "GPL-2.0-or-later",
    "homepage": "https://technology.orangehive.de",
    "keywords": [
        "TYPO3 CMS",
        "Simplified Development",
        "Backend layouts",
        "Controller",
        "Model",
        "Hook",
        "Custom content element"
    ],
    "require": {
        "php": "^8.0",
        "typo3/cms-core": "^11.5||^12"
    },
    "suggest": {
        "typo3/cms-fluid-styled-content": "Required for custom content elements"
    },
    "autoload": {
        "psr-4": {
            "OrangeHive\\Simplyment\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "simplyment"
        }
    }
}
